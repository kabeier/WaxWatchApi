ENVIRONMENT=
LOG_LEVEL=
JSON_LOGS=

# CORS (use explicit origins when credentials are enabled; never use * with credentials)
CORS_ALLOWED_ORIGINS=
CORS_ALLOWED_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Authorization,Content-Type
CORS_ALLOW_CREDENTIALS=false

DATABASE_URL=
DB_POOL=

DB_POOL_SIZE= # Dev Only
DB_MAX_OVERFLOW= # Dev Only

DEV_AUTO_CREATE_USERS=
DEV_BACKFILL_ON_RULE_CHANGE= # Dev Only
DEV_BACKFILL_DAYS= # Dev Only
DEV_BACKFILL_LIMIT=
PROVIDER_FORCE_MOCK= # Opt. For Testing

DISCOGS_USER_AGENT=
DISCOGS_TOKEN=

# Supabase auth
SUPABASE_URL= # Optional if AUTH_ISSUER + AUTH_JWKS_URL are set
AUTH_ISSUER=
AUTH_AUDIENCE=authenticated
AUTH_JWKS_URL=
AUTH_JWT_ALGORITHMS=["ES256"]
AUTH_JWKS_CACHE_TTL_SECONDS=300
AUTH_CLOCK_SKEW_SECONDS=30
# Local test helpers
# Tests use PyJWT/cryptography from requirements-dev.txt
# Example: PYENV_VERSION=3.11.14 make bootstrap-test-deps
PYENV_VERSION=

# Token crypto (encrypt ExternalAccountLink.access_token at rest)
TOKEN_CRYPTO_KMS_KEY_ID= # optional key identifier for envelope metadata
TOKEN_CRYPTO_LOCAL_KEY_PATH= # path to a Fernet key file used in local/dev
TOKEN_CRYPTO_LOCAL_KEY= # optional inline Fernet key (prefer key path)
